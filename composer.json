{
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/michalmaterniak/zadmor"
        }
    ],
    "require": {
        "commitm/zadmor": "^0.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.0",
        "phpunit/phpunit": "11.0.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "tests/",
            "TestMichalM\\": "vendor/commitm/zadmor/tests/"
        }
    }
}
